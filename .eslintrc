{
  "extends": "airbnb",
  "rules": {
    "react/jsx-no-bind": 0,
    "react/jsx-uses-react": 1,
    "arrow-body-style": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "brace-style": [0, "1tbs"]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
